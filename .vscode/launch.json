{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug Renderer Process",
            "type": "chrome",
            "request": "launch",
            "webRoot": "${workspaceRoot}/src",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
            "linux": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron"
            },
            "osx": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/electron-prebuilt/dist/electron.app/Contents/MacOS/Electron"
            },
            "runtimeArgs": [
                "${workspaceRoot}/src/main.js",
                "--enable-logging",
                "--remote-debugging-port=9222",
                "--openDevTools"
            ],
            "sourceMaps": false
        },
        {
            "name": "Launch",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/src/main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
           "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
            "linux": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron"
            },
            "osx": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/electron-prebuilt/dist/electron.app/Contents/MacOS/Electron"
            },
            
            "runtimeArgs": [
                "--nolazy",
                "--openDevTools"
            ],
            "env": {
                "NODE_ENV": "development",
                "COINS_ENV": "development"
                //,
                // "ATOM_SHELL_INTERNAL_RUN_AS_NODE": "0"
            },
            "preLaunchTask": "",
            "sourceMaps": false,
            "protocol" : "inspector"
            
        },
        {
            "name": "Run Test script renderer debug",
            "type": "chrome",
            "request": "launch",
            "webRoot": "${workspaceRoot}/src",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
            "linux": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron"
            },
            "osx": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/electron-prebuilt/dist/electron.app/Contents/MacOS/Electron"
            },
            "runtimeArgs": [
                "${workspaceRoot}/src/main.js",
                "--enable-logging",
                "--remote-debugging-port=9222",
                "--runalltests",
                "--openDevTools"
            ],
            "sourceMaps": false
        },
        {
            "name": "Run Test script Debug Main",
            "type": "node",
            "request": "launch",
            "program": "${workspaceRoot}/src/main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
           "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "windows": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd"
            },
            "linux": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron"
            },
            "osx": {
                "runtimeExecutable": "${workspaceRoot}/node_modules/electron-prebuilt/dist/electron.app/Contents/MacOS/Electron"
            },
            
            "runtimeArgs": [
                "--nolazy",
                "--runalltests",
                "--openDevTools"
                
            ],
            "env": {
                "NODE_ENV": "development",
                "COINS_ENV": "development"
                //,
                // "ATOM_SHELL_INTERNAL_RUN_AS_NODE": "0"
            },
            "preLaunchTask": "",
            "sourceMaps": false,
            "protocol" : "inspector"
            
        }
    ]
}